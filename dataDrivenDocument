       .attr("x", (d, i) => i * 30)
       .attr("y", (d, i) => h - 3 * d)
       .attr("width", 25)
       .attr("height", (d, i) => 3 * d)
       .attr("fill", "navy");

    svg.selectAll("text")
       .data(dataset)
       .enter()
       // Add your code below this line

.append("text").text((d) => d).attr("y", (d) => (h - 3 * d) - 3 ).attr("x",(d,i)=> i * 30)


       // Add your code above this line
  </script>
